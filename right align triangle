#include <stdio.h>

int main() {
    // Write C code here
    int r;
    scanf("%d",&r);
    for(int i=r;i>0;i--)
      {for(int j=1;j<=r;j++)
        { 
            if(j>=i)
            {printf("*");
            }
            else
            {printf(" ");}
        }
       printf("\n");
    }

    return 0;
}
