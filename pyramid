#include <stdio.h>

int main() {
    // Write C code here
    int r,c;
    scanf("%d",&r);
    c=2*r-1;
    for(int i=r;i>=1;i--)
      {for(int j=1;j<=c-i+1;j++)
        { 
            if(j>=i)
         { printf("*");}
         else
         {  printf(" "); }
        }
       printf("\n");
    }

    return 0;
}
