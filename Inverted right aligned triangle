#include <stdio.h>

int main() {
    // Write C code here
    int r;
    scanf("%d",&r);
    for(int i=r;i>0;i--)
      {for(int j=r;j>0;j--)
        { 
            if(j<=i)
            {printf("*");
            }
            else
            {printf(" ");
            }
        }
       printf("\n");
    }

    return 0;
}
